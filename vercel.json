{
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "rewrites": [
    {
      "source": "/backend/NBA/(.*)",
      "destination": "/api/NBA/$1"
    },
    {
      "source": "/backend/(.*)",
      "destination": "/api/$1"
    }
  ]
}
